{
  "cli": {
    "version": ">= 14",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "environment": "development",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "preview": {
      "channel": "preview",
      "environment": "preview",
      "env": {
        "NODE_ENV": "test"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "environment": "production",
      "env": {
        "NODE_ENV": "production"
      },
      "android": {
        "buildType": "apk"
      }
    }
  }
}
