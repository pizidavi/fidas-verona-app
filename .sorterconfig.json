{
  "root": "app",
  "organizeOnSave": true,
  "blocks": [
    {
      "name": "React",
      "libraries": ["react", "react-native"]
    },
    {
      "name": "Hooks",
      "folders": ["hooks"]
    },
    {
      "name": "Store",
      "libraries": ["zustand", "zustand/middleware"],
      "folders": ["store"]
    },
    {
      "name": "Api",
      "libraries": ["@tanstack/react-query"],
      "folders": ["api"]
    },
    {
      "name": "Views",
      "folders": ["views"]
    },
    {
      "name": "Screens",
      "folders": ["screens"]
    },
    {
      "name": "Components",
      "folders": ["components"]
    },
    {
      "name": "Navigation",
      "libraries": [
        "@react-navigation/native",
        "@react-navigation/native-stack",
        "@react-navigation/bottom-tabs"
      ]
    },
    {
      "name": "Config",
      "folders": ["config"]
